{
  "compilerOptions": {
    "jsx": "preserve",
    "baseUrl": "src",
    "paths": {
      "@components/*": ["components/*"],
			"@images/*": ["images/*"],
			"@pages/*": ["pages/*"],
			"@services/*": ["services/*"],
			"@styles/*": ["styles/*"],
			"@utils/*": ["utils/*"]
